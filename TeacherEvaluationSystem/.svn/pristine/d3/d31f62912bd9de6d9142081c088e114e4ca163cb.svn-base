package ssm.entity;

public class Permission {
	private int permission_id;
	private String permission_name;
	private String url;
	private int permission_grade;
	private int paretn_num;
	private int sort;
	private String describe;
	public int getPermission_id() {
		return permission_id;
	}
	public void setPermission_id(int permission_id) {
		this.permission_id = permission_id;
	}
	public String getPermission_name() {
		return permission_name;
	}
	public void setPermission_name(String permission_name) {
		this.permission_name = permission_name;
	}
	public String getUrl() {
		return url;
	}
	public void setUrl(String url) {
		this.url = url;
	}
	public int getPermission_grade() {
		return permission_grade;
	}
	public void setPermission_grade(int permission_grade) {
		this.permission_grade = permission_grade;
	}
	public int getParetn_num() {
		return paretn_num;
	}
	public void setParetn_num(int paretn_num) {
		this.paretn_num = paretn_num;
	}
	public int getSort() {
		return sort;
	}
	public void setSort(int sort) {
		this.sort = sort;
	}
	public String getDescribe() {
		return describe;
	}
	public void setDescribe(String describe) {
		this.describe = describe;
	}
	
}
